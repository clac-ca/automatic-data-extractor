{
  "info": {
    "schema": "ade.manifest/v0.5",
    "title": "Example Membership Config",
    "version": "1.0.0"
  },
  "env": {
    "LOCALE": "en-CA"
  },
  "secrets": {},
  "engine": {
    "defaults": {
      "timeout_ms": 120000,
      "memory_mb": 256,
      "allow_net": false
    }
  },
  "hooks": {
    "on_activate": [],
    "on_job_start": [
      {
        "script": "on_job_start.py",
        "limits": {
          "timeout_ms": 2000
        }
      }
    ],
    "on_after_extract": [],
    "on_job_end": [
      {
        "script": "on_job_end.py",
        "limits": {
          "timeout_ms": 2000
        }
      }
    ]
  },
  "columns": {
    "order": [
      "member_id",
      "member_full_name"
    ],
    "meta": {
      "member_id": {
        "label": "Member ID",
        "required": true,
        "enabled": true,
        "script": "columns/member_id.py"
      },
      "member_full_name": {
        "label": "Member Full Name",
        "required": true,
        "enabled": true,
        "script": "columns/member_full_name.py"
      }
    }
  }
}
