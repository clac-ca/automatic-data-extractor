{
  "schema": "ade.manifest/v1",
  "version": "1.6.1",
  "name": "Sandbox Minimal Config",
  "description": "Sample config mirroring the ade_engine smoke-test fixtures.",
  "script_api_version": 3,
  "columns": {
    "order": ["member_id", "value"],
    "fields": {
      "member_id": {
        "label": "Member ID",
        "module": "column_detectors.member_id",
        "required": true,
        "synonyms": ["member id", "member number", "member#"],
        "type": "string"
      },
      "value": {
        "label": "Value",
        "module": "column_detectors.value",
        "required": false,
        "synonyms": ["score", "amount"],
        "type": "string"
      }
    }
  },
  "hooks": {
    "on_run_start": ["hooks.notes"],
    "on_after_extract": [],
    "on_after_mapping": [],
    "on_before_save": [],
    "on_run_end": []
  },
  "writer": {
    "append_unmapped_columns": true,
    "unmapped_prefix": "raw_"
  },
  "extra": null
}
