{
  "schema_version": "ade.manifest@1",
  "script_api": 1,
  "config": {
    "display_name": "ADE Config Package"
  },
  "engine": {
    "defaults": {
      "detector_sample_size": 64,
      "mapping_score_threshold": 0.0
    },
    "writer": {
      "append_unmapped_columns": true,
      "unmapped_prefix": "raw_"
    }
  },
  "columns": {
    "order": ["member_id", "email", "first_name", "last_name"],
    "meta": {
      "member_id":  { "label": "Member ID",  "hidden": false },
      "email":      { "label": "Email",      "hidden": false },
      "first_name": { "label": "First Name", "hidden": false },
      "last_name":  { "label": "Last Name",  "hidden": false }
    }
  }
}