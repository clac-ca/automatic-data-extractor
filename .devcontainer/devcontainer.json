{
  "name": "automatic-data-extractor",
  "dockerComposeFile": "docker-compose.yml",
  "service": "ade",
  "workspaceFolder": "/workspaces/automatic-data-extractor",
  "shutdownAction": "stopCompose",
  "remoteUser": "vscode",
  "updateRemoteUserUID": true,
  "runServices": [
    "sql",
    "azurite"
  ],
  "forwardPorts": [
    "sql:1433",
    "azurite:10000",
    8000,
    5173
  ],
  "postCreateCommand": "bash scripts/dev/setup.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker",
        "redhat.vscode-yaml"
      ],
      "settings": {
        "python.defaultInterpreterPath": "/workspaces/automatic-data-extractor/.venv/bin/python",
        "python.terminal.activateEnvironment": true
      }
    }
  }
}
